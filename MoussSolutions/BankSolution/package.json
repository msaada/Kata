{
  "name": "node-flowtype-boilerplate",
  "version": "0.0.0",
  "description":
    "Minimalistic boilerplate to quick-start Node.js development in ES6+ (stage-3) with Flowtype checking.",
  "engineStrict": true,
  "engines": {
    "node": ">=8.9"
  },
  "devDependencies": {
    "babel-cli": "~6.26.0",
    "babel-core": "~6.26.0",
    "babel-eslint": "~8.2.2",
    "babel-jest": "~21.2.0",
    "babel-plugin-transform-async-to-generator": "~6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "~6.26.0",
    "babel-plugin-transform-flow-strip-types": "~6.22.0",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-preset-stage-3": "~6.24.1",
    "eslint": "~4.19.0",
    "eslint-config-airbnb-base": "~12.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-flow-vars": "^0.5.0",
    "eslint-plugin-flowtype": "~2.46.1",
    "eslint-plugin-import": "~2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "~0.68.0",
    "jest": "~21.2.1"
  },
  "scripts": {
    "clean": "rm -rf coverage dist tmp",
    "postlint": "npm run typecheck",
    "typecheck": "flow check",
    "test": "npm run test-only",
    "test-only": "jest --coverage",
    "test:watch": "jest --watch",
    "build": "babel src/**/*.js -d dist",
    "build:watch": "babel --watch src/**/*.js -d dist"
  },
  "author":
    "Jakub Synowiec <github@jakubsynowiec.info> (https://jakubsynowiec.info)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsynowiec/node-flowtype-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/jsynowiec/node-flowtype-boilerplate/issues"
  },
  "homepage": "https://github.com/jsynowiec/node-flowtype-boilerplate#readme",
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": ["js"],
    "coverageDirectory": "coverage",
    "collectCoverageFrom": ["src/**/*.js"]
  },
  "dependencies": {
    "babel-runtime": "~6.26.0",
    "moment": "^2.21.0"
  }
}