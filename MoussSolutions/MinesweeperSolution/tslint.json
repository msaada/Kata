{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "quotemark": [true, "single", "avoid-escape", "avoid-template"],
      "interface-name" : [true, "never-prefix"],
      "arrow-parens": [true, "ban-single-arg-parens"],
      "no-console": [false],
      "trailing-comma": [
        true,
        {
          "multiline": {
            "objects": "always",
            "arrays": "always",
            "functions": "never",
            "typeLiterals": "ignore"
          },
          "esSpecCompliant": true
        }
      ]
    },
    "rulesDirectory": []
}